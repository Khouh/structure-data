BEGIN

//somme de deux tableaux

sum:=0;
FOR i FROM 0 TO tab1length STEP 1  DO
    sum=sum+tab1[i]+tab2[i]
END_FOR
IF tab1[i]<>tab2[i] 
THEN write ("error")


//la somme de deux tableux distinct

FOR i FROM 0 TO tab1length1 STEP 1  DO
found := false;
FOR j FROM 0 TO tab2length2 STEP 1  DO
IF tab1[i]=tab2[i] 
found := true
break;
IF found!=true
s=s+tab1[i]
END_FOR

FOR i FROM 0 TO tab2length2 STEP 1  DO
found := false;
FOR j FROM 0 TO tab1length1 STEP 1  DO
IF tab1[i]=tab2[i] 
